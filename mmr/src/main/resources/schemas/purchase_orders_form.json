    {
      "id": "cmd-uid-04",
      "name": "cmd-delete-user",
      "description": "创建用户对象命令",
      "delegate": "FilterDataCommand",
      "filter":  {
        "op": "AND",
        "values":[
          {"op":"=", "param":"name_"},
          {"op":">", "param":"name_"},
          {"op":"OR", "values": [
              {"op": "CONTAINS", "param":"name_"},
              {"op":">", "param":"age_"}
            ]
          }
        ]
      }
    }
