      "说明":"对于数据集，总是有CURD And Filter基本操作，其他都是基于这些操作的组合",
      "commands": []
    },
    {
      "id": "model-basic-item",
      "name": "SAMPLE_USER_INFO_ITEM",
      "description": "用户明细对象",
      "attributes": [
        { "id":"aid01", "name":"id_", "description":"标识", "valueType":"STRING", "insertable":false, "updateable":false, "primary": true },
        { "id":"aid02", "name":"name_", "description":"姓名", "valueType":"STRING", "insertable":true, "updateable":true, "primary": false },
        { "id":"aid03", "name":"age_", "description":"年龄", "valueType":"LONG", "insertable":true, "updateable":true, "primary": false }
      ],
      "说明":"对于数据集，总是有CURD And Filter基本操作，其他都是基于这些操作的组合",
      "commands": []
