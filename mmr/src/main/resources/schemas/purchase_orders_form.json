        { "id":"aid01", "name":"id_", "description":"标识", "valueType":"STRING", "insertable":true, "updateable":true, "primary": true },
        { "id":"aid02", "name":"name_", "description":"姓名", "valueType":"STRING", "insertable":true, "updateable":true, "primary": false },
